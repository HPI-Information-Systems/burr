@prefix base: <file:///base/> .
@prefix d2rq: <http://www.wiwiss.fu-berlin.de/suhl/bizer/D2RQ/0.1#> .
@prefix jdbc: <http://d2rq.org/terms/jdbc/> .
@prefix map: <http://mapping/> .

map:hasCid_ClassMap_student_course_instructor_ClassMap_course a d2rq:PropertyBridge ;
    d2rq:belongsToClassMap map:ClassMap_student_course_instructor ;
    d2rq:join "course.id   =   student_course_instructor.cid" ;
    d2rq:property base:hasCid ;
    d2rq:refersToClassMap map:ClassMap_course .

map:hasCredits_ClassMap_course_None a d2rq:PropertyBridge ;
    d2rq:belongsToClassMap map:ClassMap_course ;
    d2rq:column "course.credits" ;
    d2rq:property base:hasCredits .

map:hasId_ClassMap_course_None a d2rq:PropertyBridge ;
    d2rq:belongsToClassMap map:ClassMap_course ;
    d2rq:column "course.id" ;
    d2rq:property base:hasId .

map:hasId_ClassMap_instructor_None a d2rq:PropertyBridge ;
    d2rq:belongsToClassMap map:ClassMap_instructor ;
    d2rq:column "instructor.id" ;
    d2rq:property base:hasId .

map:hasId_ClassMap_student_None a d2rq:PropertyBridge ;
    d2rq:belongsToClassMap map:ClassMap_student ;
    d2rq:column "student.id" ;
    d2rq:property base:hasId .

map:hasIid_ClassMap_student_course_instructor_ClassMap_instructor a d2rq:PropertyBridge ;
    d2rq:belongsToClassMap map:ClassMap_student_course_instructor ;
    d2rq:join "instructor.id   =   student_course_instructor.iid" ;
    d2rq:property base:hasIid ;
    d2rq:refersToClassMap map:ClassMap_instructor .

map:hasMax_students_ClassMap_course_None a d2rq:PropertyBridge ;
    d2rq:belongsToClassMap map:ClassMap_course ;
    d2rq:column "course.max_students" ;
    d2rq:property base:hasMax_students .

map:hasName_ClassMap_course_None a d2rq:PropertyBridge ;
    d2rq:belongsToClassMap map:ClassMap_course ;
    d2rq:column "course.name" ;
    d2rq:property base:hasName .

map:hasName_ClassMap_instructor_None a d2rq:PropertyBridge ;
    d2rq:belongsToClassMap map:ClassMap_instructor ;
    d2rq:column "instructor.name" ;
    d2rq:property base:hasName .

map:hasName_ClassMap_student_None a d2rq:PropertyBridge ;
    d2rq:belongsToClassMap map:ClassMap_student ;
    d2rq:column "student.name" ;
    d2rq:property base:hasName .

map:hasSid_ClassMap_student_course_instructor_None a d2rq:PropertyBridge ;
    d2rq:belongsToClassMap map:ClassMap_student_course_instructor ;
    d2rq:column "student_course_instructor.sid" ;
    d2rq:property base:hasSid .

map:ClassMap_student a d2rq:ClassMap ;
    d2rq:class base:student ;
    d2rq:dataStorage map:database .

map:ClassMap_instructor a d2rq:ClassMap ;
    d2rq:class base:instructor ;
    d2rq:dataStorage map:database ;
    d2rq:uriPattern "/@@instructor.id@@/" .

map:ClassMap_student_course_instructor a d2rq:ClassMap ;
    d2rq:class base:student_course_instructor ;
    d2rq:dataStorage map:database ;
    d2rq:uriPattern "/@@student_course_instructor.sid@@/@@student_course_instructor.iid@@/@@student_course_instructor.cid@@/" .

map:database a d2rq:Database ;
    jdbc:keepAlive "3600" ;
    d2rq:jdbcDSN "jdbc:postgresql://localhost:5432/nm_tables__trinary_relation__student_instructor_1" ;
    d2rq:jdbcDriver "org.postgresql.Driver" ;
    d2rq:username "lukaslaskowski" .

map:ClassMap_course a d2rq:ClassMap ;
    d2rq:class base:course ;
    d2rq:dataStorage map:database ;
    d2rq:uriPattern "/@@course.id@@/" .

