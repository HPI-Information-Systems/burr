{
    "classes": [
        {
            "id": "sources/@@river.name@@",
            "class": "Source"
        }
    ],
    "object_properties": [
        {
            "property": "locatedIn",
            "belongsToClass": "Source",
            "refersToClass": "Country",
            "join": ["@@geo_source.river@@ = @@river.name@@", "@@geo_source.country@@ = @@country.code@@"]
        },
        {
            "property": "locatedIn",
            "belongsToClass": "Estuary",
            "refersToClass": "Province",
            "join": ["geo_source.river = river.name", "geo_source.province = province.name"]
        },
        {
            "property": "inMountains",
            "belongsToClass": "Source",
            "refersToClass": "MountainRange",
            "join": ["river.mountains = mountain.mountains"]
        }
    ],
    "data_properties": [
        {
            "property": "elevation",
            "belongsToClass": "Source",
            "type": "decimal",
            "column": "@@river.sourceelevation@@"
        },
        {
            "property": "latitude",
            "belongsToClass": "Source",
            "type": "decimal",
            "column": "@@river.source.latitude@@"
        },
        {
            "property": "longitude",
            "belongsToClass": "Source",
            "type": "decimal",
            "column": "@@river.source.longitude@@"
        }
    ]
}