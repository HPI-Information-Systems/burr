{
    "classes": [
        {
            "id": "countries/@@Province.Country@@/provinces/@@Province.Name@@",
            "class": "Province"
        }
    ],
    "object_properties": [],
    "data_properties": [
        {
            "property": "name",
            "belongsToClass": "Province",
            "type": "string",
            "column": "@@Province.Name@@"
        },
        {
            "property": "area",
            "belongsToClass": "Province",
            "type": "decimal",
            "column": "@@Province.Area@@"
        },
        {
            "property": "population",
            "belongsToClass": "Province",
            "type": "integer",
            "column": "@@Province.Population@@"
        },
        {
            "property": "othername",
            "belongsToClass":"Province",
            "type": "string",
            "column": "@@provinceothername.othername@@",
            "join": ["@@province.name@@ = @@provinceothername.province@@", "@@province.country@@ = @@provinceothername.country@@"]
        },
        {
            "property": "localname",
            "belongsToClass":"Province",
            "type": "string",
            "column": "@@provincelocalname.localname@@",
            "join": ["@@province.name@@ = @@provincelocalname.province@@", "@@province.country@@ = @@provincelocalname.country@@"]
        }
    ]
}