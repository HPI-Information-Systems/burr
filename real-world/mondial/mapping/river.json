{
    "classes": [
        {
            "id": "rivers/@@river.name@@",
            "class": "River",
            "name": "River1"
        },
        {
            "id": "rivers/@@river.river",
            "class": "River",
            "name": "River2"
        }
    ],
    "object_properties": [
        {
            "property": "locatedIn",
            "belongsToClass": "River",
            "refersToClass": "Country",
            "join": [
                "geo_river.river = river.name",
                "geo_river.country = country.code"
            ]
        },
        {
            "property": "locatedIn",
            "belongsToClass": "River",
            "refersToClass": "Province",
            "join": [
                "geo_river.river = river.name",
                "geo_river.province = province.name"
            ]
        },
        {
            "property": "flowsThrough",
            "belongsToClass": "River",
            "refersToClass": "Lake",
            "join": [
                "riverthrough.lake = lake.name",
                "riverthrough.river = river.name"
            ]
        },
        {
            "property": "flowsInto",
            "belongsToClassMap": "River1",
            "refersToClassMap": "River2"
        },
        {
            "property": "flowsInto",
            "belongsToClass": "River",
            "refersToClass": "Sea",
            "join": [
                "river.sea = sea.name"
            ]
        },
        {
            "property": "flowsInto",
            "belongsToClass": "River",
            "refersToClass": "Lake",
            "join": [
                "river.lake = lake.name"
            ]
        },
        {
            "property": "hasSource",
            "belongsToClassMap": "River1",
            "refersToClass": "Source",
            "join": [
                "river.source = source.river"
            ]
        },
        {
            "property": "locatedOnIsland",
            "belongsToClass": "Mountain",
            "refersToClass": "River",
            "join": [
                "river.name = riveronisland.river",
                "riveronisland.island = island.name"
            ]
        }
    ],
    "data_properties": [
        {
            "property": "name",
            "belongsToClass": "River",
            "type": "string",
            "column": "river.name"
        },
        {
            "property": "area",
            "belongsToClass": "River",
            "type": "decimal",
            "column": "river.area"
        },
        {
            "property": "length",
            "belongsToClass": "River",
            "type": "decimal",
            "column": "river.length"
        }
    ]
}