{
    "classes": [
        {
            "id": "mountains/@@mountain.name@@",
            "class": "Mountain"
        },
        {
            "id": "mountainRanges/@@mountain.mountains@@",
            "class": "MountainRange"
        }
    ],
    "object_properties": [
        {
            "property": "inMountains",
            "belongsToClass": "Mountain",
            "refersToClass": "MountainRange"
        },
        {
            "property": "locatedIn",
            "belongsToClass": "Mountain",
            "refersToClass": "Country",
            "join": ["@@geo_mountain.mountain@@ = @@mountain.name@@", "@@geo_mountain.country@@ = @@country.code@@"]
        },
        {
            "property": "locatedIn",
            "belongsToClass": "Mountain",
            "refersToClass": "Province",
            "join": ["@@geo_mountain.mountain@@ = @@mountain.name@@", "@@geo_mountain.province@@ = @@province.name@@"]
        },
        {
            "property": "locatedOnIsland",
            "belongsToClass": "Mountain",
            "refersToClass": "Island",
            "join": ["mountain.name = mountainonisland.mountain", "mountainonisland.island = island.name"]
        }
    ],
    "data_properties": [
        {
            "property": "type",
            "belongsToClass": "Mountain",
            "type": "string",
            "column": "@@mountain.type@@"
        },
        {
            "property": "name",
            "belongsToClass": "MountainRange",
            "type": "string",
            "column": "@@mountain.mountains@@"
        },
        {
            "property": "name",
            "belongsToClass": "Mountain",
            "type": "string",
            "column": "@@mountain.name@@"
        },
        {
            "property": "latitude",
            "belongsToClass": "Mountain",
            "type": "decimal",
            "column": "@@mountain.latitude@@"
        },
        {
            "property": "longitude",
            "belongsToClass": "Mountain",
            "type": "decimal",
            "column": "@@mountain.longitude@@"
        },
        {
            "property": "elevation",
            "belongsToClass": "Mountain",
            "type": "decimal",
            "column": "@@mountain.elevation@@"
        }
    ]
}