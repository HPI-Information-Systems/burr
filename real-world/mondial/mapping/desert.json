{
    "classes": [
        {
            "id": "deserts/@@desert.name@@",
            "class": "Desert"
        }
    ],
    "object_properties": [
        {
            "property": "locatedIn",
            "belongsToClass": "Desert",
            "refersToClass": "Country",
            "join": [
                "geo_desert.desert = desert.name",
                "geo_desert.country = country.code"
            ]
        },
        {
            "property": "locatedIn",
            "belongsToClass": "Desert",
            "refersToClass": "Province",
            "join": [
                "geo_desert.desert = desert.name",
                "geo_desert.province = province.name"
            ]
        }
    ],
    "data_properties": [
        {
            "property": "name",
            "belongsToClass": "Desert",
            "type": "string",
            "column": "desert.name"
        },
        {
            "property": "latitude",
            "belongsToClass": "Desert",
            "type": "decimal",
            "column": "desert.latitude"
        },
        {
            "property": "longitude",
            "belongsToClass": "Desert",
            "type": "decimal",
            "column": "desert.longitude"
        },
        {
            "property": "area",
            "belongsToClass": "Desert",
            "type": "decimal",
            "column": "desert.area"
        }
    ]
}