{
    "classes": [
        {
            "id": "estuarys/@@river.name@@",
            "class": "Estuary"
        }
    ],
    "object_properties": [
        {
            "property": "locatedIn",
            "belongsToClass": "Estuary",
            "refersToClass": "Country",
            "join": ["geo_estuary.river = river.name", "geo_estuary.country = country.code"]
        },
        {
            "property": "locatedIn",
            "belongsToClass": "Estuary",
            "refersToClass": "Province",
            "join": ["geo_estuary.river = river.name", "geo_estuary.province = province.name"]
        }
    ],
    "data_properties": [
        {
            "property": "elevation",
            "belongsToClass": "Estuary",
            "type": "decimal",
            "column": "@@river.estuaryelevation@@"
        },
        {
            "property": "latitude",
            "belongsToClass": "Estuary",
            "type": "decimal",
            "column": "@@river.estuary.latitude@@"
        },
        {
            "property": "longitude",
            "belongsToClass": "Source",
            "type": "decimal",
            "column": "@@river.estuary.longitude@@"
        }
    ]
}