{
    "classes": [
        {
            "id": "Encompassed__Continent_@@encompasses.continent@@__Country_@@encompasses.country@@",
            "class": "Encompassed"
        }
    ],
    "object_properties": [
        {
            "property": "encompassedBy",
            "belongsToClass": "Encompassed",
            "refersToClass": "Continent",
            "join": ["@@encompasses.continent@@ = @@continent.name@@"]
        },
        {
            "property": "encompassedArea",
            "belongsToClass": "Encompassed",
            "refersToClass": "Country",
            "join": ["@@encompasses.country@@ = @@country.code@@"]
        }
    ],
    "data_properties": [
        {
            "property": "percent",
            "belongsToClass": "Encompassed",
            "type": "decimal",
            "column": "@@encompasses.percentage@@"
        }
    ]
}