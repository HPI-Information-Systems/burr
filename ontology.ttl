### Datatype Properties
base:area a owl:DatatypeProperty ;
    rdfs:domain [ a owl:Class ] ;
    rdfs:range xsd:float ;
    rdfs:label "area" ;
    skos:definition "The area of a geographical entity." .

base:population a owl:DatatypeProperty ;
    rdfs:domain [ a owl:Class ] ;
    rdfs:range xsd:integer ;
    rdfs:label "population" ;
    skos:definition "The population of a geographical entity." .

base:elevation a owl:DatatypeProperty ;
    rdfs:domain [ a owl:Class ] ;
    rdfs:range xsd:float ;
    rdfs:label "elevation" ;
    skos:definition "The elevation of a geographical entity." .

base:latitude a owl:DatatypeProperty ;
    rdfs:domain [ a owl:Class ] ;
    rdfs:range xsd:float ;
    rdfs:label "latitude" ;
    skos:definition "The latitude of a geographical entity." .

base:longitude a owl:DatatypeProperty ;
    rdfs:domain [ a owl:Class ] ;
    rdfs:range xsd:float ;
    rdfs:label "longitude" ;
    skos:definition "The longitude of a geographical entity." .

base:percentage a owl:DatatypeProperty ;
    rdfs:domain [ a owl:Class ] ;
    rdfs:range xsd:float ;
    rdfs:label "percentage" ;
    skos:definition "The percentage value related to a geographical entity." .

base:depth a owl:DatatypeProperty ;
    rdfs:domain [ a owl:Class ] ;
    rdfs:range xsd:float ;
    rdfs:label "depth" ;
    skos:definition "The depth of a geographical entity." .

base:year a owl:DatatypeProperty ;
    rdfs:domain [ a owl:Class ] ;
    rdfs:range xsd:gYear ;
    rdfs:label "year" ;
    skos:definition "The year associated with a geographical entity." .

