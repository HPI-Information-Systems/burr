@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix base: <https://base_ontology.com#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix schema: <http://schema.org/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .

<#TriplesMapBook> a rr:TriplesMap;
    rr:logicalTable [ rr:tableName "Book" ];
    rr:subjectMap [
        rr:template "https://base_ontology.com#Book/{BookID}";
        rr:class base:Book;
    ];
    rr:predicateObjectMap [
        rr:predicate base:non_null_count;
        rr:objectMap [ rr:column "non_null_count" ];
    ];
    rr:predicateObjectMap [
        rr:predicate base:type;
        rr:objectMap [ rr:column "type" ];
    ];
    rr:predicateObjectMap [
        rr:predicate base:unique_count;
        rr:objectMap [ rr:column "unique_count" ];
    ];
    rr:predicateObjectMap [
        rr:predicate base:count;
        rr:objectMap [ rr:column "count" ];
    ];
    rr:predicateObjectMap [
        rr:predicate base:max;
        rr:objectMap [ rr:column "max" ];
    ];
    rr:predicateObjectMap [
        rr:predicate base:mean;
        rr:objectMap [ rr:column "mean" ];
    ];
    rr:predicateObjectMap [
        rr:predicate base:min;
        rr:objectMap [ rr:column "min" ];
    ];
    rr:predicateObjectMap [
        rr:predicate base:std;
        rr:objectMap [ rr:column "std" ];
    ];
    rr:predicateObjectMap [
        rr:predicate base:hasAddress;
        rr:objectMap [ rr:parentTriplesMap <#TriplesMapAddress> ];
    ];
    rr:predicateObjectMap [
        rr:predicate base:hasAuthor;
        rr:objectMap [ rr:parentTriplesMap <#TriplesMapAuthor> ];
    ];
    rr:predicateObjectMap [
        rr:predicate base:hasBirthYear;
        rr:objectMap [ rr:parentTriplesMap <#TriplesMapBirthYear> ];
    ];
    rr:predicateObjectMap [
        rr:predicate base:hasCopiesAvailable;
        rr:objectMap [ rr:parentTriplesMap <#TriplesMapCopiesAvailable> ];
    ];
    rr:predicateObjectMap [
        rr:predicate base:hasISBN;
        rr:objectMap [ rr:parentTriplesMap <#TriplesMapISBN> ];
    ];
    rr:predicateObjectMap [
        rr:predicate base:hasName;
        rr:objectMap [ rr:parentTriplesMap <#TriplesMapName> ];
    ];
    rr:predicateObjectMap [
        rr:predicate base:hasNationality;
        rr:objectMap [ rr:parentTriplesMap <#TriplesMapNationality> ];
    ];
    rr:predicateObjectMap [
        rr:predicate base:hasPublicationYear;
        rr:objectMap [ rr:parentTriplesMap <#TriplesMapPublicationYear> ];
    ].

<#TriplesMapAddress> a rr:TriplesMap;
    rr:logicalTable [ rr:tableName "Address" ];
    rr:subjectMap [
        rr:template "https://base_ontology.com#Address/{AddressID}";
        rr:class schema:PostalAddress;
    ].

<#TriplesMapAuthor> a rr:TriplesMap;
    rr:logicalTable [ rr:tableName "Author" ];
    rr:subjectMap [
        rr:template "https://base_ontology.com#Author/{AuthorID}";
        rr:class foaf:Person;
    ].

<#TriplesMapBirthYear> a rr:TriplesMap;
    rr:logicalTable [ rr:tableName "BirthYear" ];
    rr:subjectMap [
        rr:template "https://base_ontology.com#BirthYear/{BirthYearID}";
        rr:class dc:date;
    ].

<#TriplesMapCopiesAvailable> a rr:TriplesMap;
    rr:logicalTable [ rr:tableName "CopiesAvailable" ];
    rr:subjectMap [
        rr:template "https://base_ontology.com#CopiesAvailable/{CopiesAvailableID}";
        rr:class schema:Book;
    ].

<#TriplesMapISBN> a rr:TriplesMap;
    rr:logicalTable [ rr:tableName "ISBN" ];
    rr:subjectMap [
        rr:template "https://base_ontology.com#ISBN/{ISBNID}";
        rr:class dc:identifier;
    ].

<#TriplesMapName> a rr:TriplesMap;
    rr:logicalTable [ rr:tableName "Name" ];
    rr:subjectMap [
        rr:template "https://base_ontology.com#Name/{NameID}";
        rr:class foaf:name;
    ].

<#TriplesMapNationality> a rr:TriplesMap;
    rr:logicalTable [ rr:tableName "Nationality" ];
    rr:subjectMap [
        rr:template "https://base_ontology.com#Nationality/{NationalityID}";
        rr:class foaf:nationality;
    ].

<#TriplesMapPublicationYear> a rr:TriplesMap;
    rr:logicalTable [ rr:tableName "PublicationYear" ];
    rr:subjectMap [
        rr:template "https://base_ontology.com#PublicationYear/{PublicationYearID}";
        rr:class dc:date;
    ].