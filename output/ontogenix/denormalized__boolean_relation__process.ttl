@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix base: <https://base_ontology.com#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix schema: <http://schema.org/> .

<#TriplesMapService> a rr:TriplesMap;
    rr:logicalTable [ rr:tableName "ServiceTable" ];
    rr:subjectMap [
        rr:template "https://base_ontology.com#Service/{id}";
        rr:class base:Service;
    ];
    rr:predicateObjectMap [
        rr:predicate base:id;
        rr:objectMap [ rr:column "id" ];
    ];
    rr:predicateObjectMap [
        rr:predicate base:name;
        rr:objectMap [ rr:column "name" ];
    ].

<#TriplesMapID> a rr:TriplesMap;
    rr:logicalTable [ rr:tableName "ServiceTable" ];
    rr:subjectMap [
        rr:template "https://base_ontology.com#Service/{id}";
        rr:class base:Service;
    ];
    rr:predicateObjectMap [
        rr:predicate dcterms:identifier;
        rr:objectMap [ rr:column "id" ];
    ].

<#TriplesMapName> a rr:TriplesMap;
    rr:logicalTable [ rr:tableName "ServiceTable" ];
    rr:subjectMap [
        rr:template "https://base_ontology.com#Service/{id}";
        rr:class base:Service;
    ];
    rr:predicateObjectMap [
        rr:predicate schema:name;
        rr:objectMap [ rr:column "name" ];
    ].