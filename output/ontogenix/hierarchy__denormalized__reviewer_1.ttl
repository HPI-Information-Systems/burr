@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix base: <https://base_ontology.com#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<#TriplesMapPerson> a rr:TriplesMap;
    rr:logicalTable [ rr:tableName "PersonTable" ];
    rr:subjectMap [
        rr:template "https://base_ontology.com#Person/{PersonID}";
        rr:class base:Person;
    ];
    rr:predicateObjectMap [
        rr:predicate base:count;
        rr:objectMap [ rr:column "count"; rr:datatype xsd:integer ];
    ];
    rr:predicateObjectMap [
        rr:predicate base:unique;
        rr:objectMap [ rr:column "unique"; rr:datatype xsd:integer ];
    ].