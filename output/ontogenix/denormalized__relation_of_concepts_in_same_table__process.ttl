@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix base: <https://base_ontology.com#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<#TriplesMapProcess> a rr:TriplesMap;
    rr:logicalTable [ rr:tableName "ProcessTable" ];
    rr:subjectMap [
        rr:template "https://base_ontology.com#Process/{process_id}";
        rr:class base:Process;
    ];
    rr:predicateObjectMap [
        rr:predicate base:hasProcessID;
        rr:objectMap [
            rr:parentTriplesMap <#TriplesMapProcessID>;
            rr:column "process_id";
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate base:hasAreaDescription;
        rr:objectMap [
            rr:parentTriplesMap <#TriplesMapAreaDescription>;
            rr:column "area_description";
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate base:hasAreaName;
        rr:objectMap [
            rr:parentTriplesMap <#TriplesMapAreaName>;
            rr:column "area_name";
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate base:hasBusinessAreaID;
        rr:objectMap [
            rr:parentTriplesMap <#TriplesMapBusinessAreaID>;
            rr:column "business_area_id";
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate base:hasBusinessCapabilityID;
        rr:objectMap [
            rr:parentTriplesMap <#TriplesMapBusinessCapabilityID>;
            rr:column "business_capability_id";
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate base:hasCapabilityDescription;
        rr:objectMap [
            rr:parentTriplesMap <#TriplesMapCapabilityDescription>;
            rr:column "capability_description";
        ]
    ];
    rr:predicateObjectMap [
        rr:predicate base:hasCapabilityName;
        rr:objectMap [
            rr:parentTriplesMap <#TriplesMapCapabilityName>;
            rr:column "capability_name";
        ]
    ].

