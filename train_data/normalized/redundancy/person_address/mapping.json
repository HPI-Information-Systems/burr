{
    "classes": [
        {
            "id": "@@person.id@@",
            "class": "Person",
            "name": "Person"
        },
        {
            "id": "@@person.department_id@@",
            "class": "Department",
            "name": "Department1"
        },
        {
            "id": "@@cost_center.department_id@@",
            "class": "Department",
            "name": "Department2"
        },
        {
            "id": "@@person.zip@@",
            "class": "Location",
            "name": "Location"
        },
        {
            "id": "@@cost_center.id@@",
            "class": "CostCenter",
            "name": "CostCenter"
        }
    ],
    "object_properties": [
        {
            "property": "worksIn",
            "belongsToClassMap": "Person",
            "refersToClassMap": "Department"
        },
        {
            "property": "belongs_to",
            "belongsToClassMap": "CostCenter",
            "refersToClassMap": "Department",
            "join": [
                "cost_center.department_id = department.id"
            ]
        },
        {
            "property": "livesIn",
            "belongsToClassMap": "Person",
            "refersToClassMap": "Location"
        }
    ],
    "data_properties": [
        {
            "property": "first_name",
            "belongsToClassMap": "Person",
            "type": "string",
            "column": "person.first_name"
        },
        {
            "property": "last_name",
            "belongsToClassMap": "Person",
            "type": "string",
            "column": "person.last_name"
        },
        {
            "property": "email",
            "belongsToClassMap": "Person",
            "type": "string",
            "column": "person.email"
        },
        {
            "property": "name",
            "belongsToClassMap": "Department1",
            "type": "string",
            "column": "person.department"
        }, 
        {
            "property": "name",
            "belongsToClassMap": "Department2",
            "type": "string",
            "column": "cost_center.department"
        }, 
        {
            "property": "name",
            "belongsToClassMap": "CostCenter",
            "type": "string",
            "column": "cost_center.name"
        },
        {
            "property": "street",
            "belongsToClassMap": "Person",
            "type": "string",
            "column": "person.street"
        },
        {
            "property": "house_number",
            "belongsToClassMap": "Person",
            "type": "string",
            "column": "person.house_number"
        },
        {
            "property": "zip_code",
            "belongsToClassMap": "City",
            "type": "string",
            "column": "person.zip"
        },
        {
            "property": "name",
            "belongsToClassMap": "City",
            "type": "string",
            "column": "person.city"
        },
        {
            "property": "state",
            "belongsToClassMap": "City",
            "type": "string",
            "column": "person.state"
        }
    ]
}