{
    "classes": [
        {
            "id": "@@order.product_name@@",
            "class": "Product",
            "name": "Product"
        },
        {
            "id": "@@client.id@@",
            "class": "Client",
            "name": "Client"
        }
    ],
    "object_properties": [
        {
            "property": "orders",
            "belongsToClassMap": "Client",
            "refersToClassMap": "Product",
            "join": [
                "client.id = order.client_id"
            ]
        }
    ],
    "data_properties": [
        {
            "property": "name",
            "belongsToClassMap": "Client",
            "type": "string",
            "column": "client.name"
        },
        {
            "property": "address",
            "belongsToClassMap": "Client",
            "type": "string",
            "column": "client.address"
        },
        {
            "property": "name",
            "belongsToClassMap": "Product",
            "type": "string",
            "column": "order.product_name"
        },
        {
            "property": "price",
            "belongsToClassMap": "Product",
            "type": "integer",
            "column": "order.price"
        }
    ]
}