{
    "classes": [
        {
            "id": "@@person.name@@",
            "class": "Person"
        },
        {
            "id": "@@receipt.id@@",
            "class": "Receipt"
        }
    ],
    "object_properties": [
        {
            "property": "receivedBy",
            "belongsToClass": "Receipt",
            "refersToClass": "Person",
            "join": ["@@person.name@@ = @@receipt.received_by@@"]
        }
    ],
    "data_properties": [
        {
            "property": "name",
            "belongsToClass": "Person",
            "type": "string",
            "column": "@@person.name@@"
        },
        {
            "property": "email",
            "belongsToClass": "Person",
            "type": "string",
            "column": "@@person.email@@"
        },
        {
            "property": "date",
            "belongsToClass": "Receipt",
            "type": "date",
            "column": "@@receipt.date@@"
        },
        {
            "property": "item",
            "belongsToClass": "Receipt",
            "type": "string",
            "column": "@@receipt.item@@"
        }
    ]
}