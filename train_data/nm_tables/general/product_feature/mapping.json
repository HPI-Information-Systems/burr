{
    "classes": [
        {
            "id": "@@product.id@@",
            "class": "Product"
        },
        {
            "id": "@@feature.id@@",
            "class": "Feature"
        }
    ],
    "object_properties": [
        {
            "property": "has_feature",
            "belongsToClass": "Product",
            "refersToClass": "Feature",
            "join": ["@@product.id@@ = @@product_feature.product_id@@", "@@feature.id@@ = @@product_feature.feature_id@@"]
        }
    ],
    "data_properties": [
        {
            "property": "name",
            "belongsToClass": "Product",
            "type": "string",
            "column": "@@product.name@@"
        },
        {
            "property": "name",
            "belongsToClass": "Feature",
            "type": "string",
            "column": "@@feature.name@@"
        }
    ]
}