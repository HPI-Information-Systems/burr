{
    "classes": [
        {
            "id": "@@employee.id@@",
            "class": "Employee"
        },
        {
            "id": "@@skill.id@@",
            "class": "Skill"
        }
    ],
    "object_properties": [
        {
            "property": "has_skill",
            "belongsToClass": "Employee",
            "refersToClass": "Skill",
            "join": ["@@employee.id@@ = @@employee_skill.employee_id@@", "@@skill.id@@ = @@employee_skill.skill_id@@"]
        }
    ],
    "data_properties": [
        {
            "property": "name",
            "belongsToClass": "Employee",
            "type": "string",
            "column": "@@employee.name@@"
        },
        {
            "property": "name",
            "belongsToClass": "Skill",
            "type": "string",
            "column": "@@skill.name@@"
        }
    ]
}